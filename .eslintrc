{
    "extends": ["airbnb-base"],
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "max-len": ["error", 120],
        "strict": 0,
        "comma-dangle": 0,
        "func-names": 0,
        "object-shorthand": 0,
        "prefer-arrow-callback": 0,
        "no-var": 0,
        "prefer-template": 0,
        "vars-on-top": 1,
        "no-underscore-dangle": 1,
        "no-param-reassign": 1
    },
    "globals": {
        "angular": false,
        "d3": false,
        "moment": false
    }
}
